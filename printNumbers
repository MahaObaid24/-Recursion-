public class Java {
    static void printNumbers(int n){
        if(n==1)
              System.out.println(n) ;
        else{
             printNumbers(n-1);
            System.out.println(n);
           
      }
       
    }
    public static void main(String[] args) {
        
       printNumbers(5);
    }
}
from 1 to n;


public class Java {
    static void printNumbers(int n){
        if(n==1)
              System.out.println(n) ;
        else{
            
            System.out.println(n);
            printNumbers(n-1);
      }
       
    }
    public static void main(String[] args) {
        
       printNumbers(5);
    }
}
from n to 1;
